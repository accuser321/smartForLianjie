    }
}
/*链接导入界面*/
.aurl {
  padding: 10px;
  padding-bottom: 30px;
  border-bottom: 1px solid #dcdcdc;
}
.url {
  border-radius: 0.1rem;
  margin-top: 0.3rem;
  border: 1px dashed #d3d3d3;
  font-size: 0.14rem;
  margin: 2%;
}
.importextarea {
    border: none !important;
    width: 100%;
