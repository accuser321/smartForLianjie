  }
}
.label_list {
  width: 15%;
  color: #c0c0c0;
  font-size: 14px;
  border: 1px solid #c0c0c0;
  border-radius: 8px;
  height: 0.68rem;
  line-height: 0.68rem;
  text-align: center;
  float: left;
  margin: 20px 1.5% 0 0;
  -webkit-box-sizing: border-box;
  box-sizing: border-box;
}
